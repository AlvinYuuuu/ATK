# Example .env file for AI Tech Consultant Agent

# Google API Keys
GOOGLE_API_KEY=your-google-api-key-here

# Mem0 Configuration
MEM0_API_KEY=your-mem0-api-key-here
MEM0_ENV=production
MEM0_HOST=http://localhost:8080

# Langfuse Configuration
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key-here
LANGFUSE_SECRET_KEY=your-langfuse-secret-key-here
LANGFUSE_HOST=https://cloud.langfuse.com

# OpenTelemetry (optional)
OTEL_EXPORTER_OTLP_ENDPOINT=
OTEL_EXPORTER_OTLP_HEADERS=

# General Settings
ENV=development
DEBUG=true

# Add any other environment variables required by your project below
